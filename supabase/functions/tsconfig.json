{
  "compilerOptions": {
    "lib": ["esnext", "dom"],
    "strict": true,
    "allowJs": true,
    "target": "ES2020",
    "module": "NodeNext",
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "checkJs": false,
    "declaration": false,
    "noEmit": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "moduleResolution": "nodenext",
    "allowImportingTsExtensions": true
  }
}
